COLUMN TABLE sclabs_alumniportal_auth_login (
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  ID NVARCHAR(36) NOT NULL,
  userid NVARCHAR(5000) NOT NULL,
  username NVARCHAR(5000) NOT NULL,
  password NVARCHAR(5000) NOT NULL,
  lastlogin NVARCHAR(5000),
  PRIMARY KEY(ID)
)